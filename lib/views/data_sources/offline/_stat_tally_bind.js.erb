<%- klass.expected_params().each do |param, value_type|
  node = param.gsub(/[:,]/, '-') -%>
$('#<%= node %>'   ).linkFrom('val',     data['<%= node %>'], 'value').linkTo('val',     data['<%= node %>'], 'value');
$('#<%= node %>-nr').linkFrom('checked', data['<%= node %>'], 'nr').linkTo('checked', data['<%= node %>'], 'nr');
<%- end -%>

