<%- Package.active.each do |package| -%>
  if (!data.<%= package.code %>)
    data.<%= package.code %> = { DeliveryRequest: '', DeliveryPickup: '' };

  <%- %w(before after).each do |visit_type| -%>
    <%- %w(DeliveryRequest DeliveryPickup).each do |pickup_type| -%>
      $('#warehouse_pickup-<%= visit_type %>-<%= package.code %>-<%= pickup_type %>').
        linkFrom('val', data.<%= package.code %>, '<%= pickup_type %>').
        linkTo('val', data.<%= package.code %>, '<%= pickup_type %>');
    <%- end -%>
  <%- end -%>
<%- end -%>
